plot <- plot + geom_point(aes(y=`Max_Weight`, colour = "Max Session Weight"))
plot <- plot + geom_line(aes(y=`30-day-1RM`, colour = "90-day-max 1RM"))
plot <- plot + labs(colour = "Parameter") + scale_colour_manual(values = c("green", "blue")) + theme(legend.position = "bottom")
